<?xml version="1.0" encoding="UTF-8"?>
<root>
	<account>
		<uid>${uid}</uid> <!-- 必填 -->	
		<appid>${appid}</appid> <!-- 选填, 没有则采用登录APP ID -->
		<token>${token}</token> <!-- 必填 -->
	</account>
	<app>
		<id>${APP_ID}</id> <!-- 必填 -->	
		<password>${APP_PASSWORD}</password> <!-- 必填 -->
	</app>
</root>